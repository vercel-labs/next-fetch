{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["prepare-env:test", "build"],
      "outputs": []
    },
    "prepare-env:test": {
      "dependsOn": ["^prepare-env:test"],
      "outputs": [],
      "cache": false
    }
  }
}
